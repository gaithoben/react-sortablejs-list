{
  "name": "react-ckeditor5",
  "version": "0.0.0-development",
  "description": "A package to put Ckeditor in your project",
  "main": "dist/index.umd.js",
  "files": ["dist", "README.md"],
  "scripts": {
    "commit": "git-cz",
    "test": "mocha src/index.test.js -w --require babel-core/register",
    "test:single": "mocha src/index.test.js --require babel-core/register",
    "prebuild": "rimraf dist",
    "build": "npm-run-all --parallel build:*",
    "build:main": "babel --copy-files --out-dir dist --ignore *.test.js src",
    "build:umd": "webpack --output-filename index.umd.js",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gaithoben/react-ckeditor5.git"
  },
  "keywords": ["kenya", "counties", "constituencies", "wards"],
  "author":
    "Bernard Gaitho <gaithoben@gmail.com> (http://github.com/gaithoben)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gaithoben/react-ckeditor5/issues"
  },
  "homepage": "https://github.com/gaithoben/react-ckeditor5#readme",
  "dependencies": {
    "react": "16.2.0"
  },
  "devDependencies": {
    "@ckeditor/ckeditor5-adapter-ckfinder": "^1.0.0-alpha.2",
    "@ckeditor/ckeditor5-alignment": "0.0.1",
    "@ckeditor/ckeditor5-autoformat": "^1.0.0-alpha.2",
    "@ckeditor/ckeditor5-basic-styles": "^1.0.0-alpha.2",
    "@ckeditor/ckeditor5-block-quote": "^1.0.0-alpha.2",
    "@ckeditor/ckeditor5-dev-utils": "^7.0.0",
    "@ckeditor/ckeditor5-dev-webpack-plugin": "^3.0.0",
    "@ckeditor/ckeditor5-easy-image": "^1.0.0-alpha.2",
    "@ckeditor/ckeditor5-editor-classic": "^1.0.0-alpha.2",
    "@ckeditor/ckeditor5-essentials": "^1.0.0-alpha.2",
    "@ckeditor/ckeditor5-heading": "^1.0.0-alpha.2",
    "@ckeditor/ckeditor5-image": "^1.0.0-alpha.2",
    "@ckeditor/ckeditor5-link": "^1.0.0-alpha.2",
    "@ckeditor/ckeditor5-list": "^1.0.0-alpha.2",
    "@ckeditor/ckeditor5-paragraph": "^1.0.0-alpha.2",
    "@ckeditor/ckeditor5-theme-lark": "^1.0.0-alpha.2",
    "@ckeditor/ckeditor5-ui": "1.0.0-alpha.2",
    "@ckeditor/ckeditor5-upload": "^1.0.0-alpha.2",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-minify-webpack-plugin": "^0.2.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-react-jsx": "6.24.1",
    "babel-preset-env": "1.6.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-es2015-webpack2": "1.0.0",
    "babel-preset-stage-2": "6.24.1",
    "babel-register": "6.26.0",
    "chai": "4.1.2",
    "css-loader": "0.28.8",
    "ghooks": "2.0.2",
    "json-loader": "0.5.7",
    "mocha": "4.0.1",
    "node-sass": "4.7.2",
    "npm-run-all": "4.1.2",
    "raw-loader": "0.5.1",
    "regenerator-runtime": "0.11.1",
    "rimraf": "2.6.2",
    "sass-loader": "6.0.6",
    "semantic-release": "^11.0.2",
    "style-loader": "0.19.1",
    "webpack": "3.10.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run test:single"
    }
  },
  "babel": {
    "presets": ["es2015"],
    "plugins": ["transform-react-jsx", "transform-class-properties"]
  }
}
